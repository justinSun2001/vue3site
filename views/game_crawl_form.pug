        th 失误&nbsp;
        th 封盖&nbsp;
        th 得分&nbsp;
        th 篮板&nbsp;
        th 助攻
        th 正负值
    - for(var j=1;j<100;j++)
      - if(playerData[j]=='首发')
        - for(var k=0;k<5;k++)
          form(action="", method="post")
            input(type='text', name='cname' required='true' size=12 value=playerData[j+15+16*k].replace(/[-\·]/,' '))
            input(type='text', name='position' required='true' size=3 value=playerData[j+16+16*k])
            input(type='text', name='game_time_min' required='true' size=3 value=playerData[j+17+16*k])
            input(type='text', name='shot_percent' required='true' size=5 value=playerData[j+18+16*k])
            input(type='text', name='three_percent' required='true' size=5 value=playerData[j+19+16*k])  
            input(type='text', name='freethrow_percent' required='true' size=5 value=playerData[j+20+16*k]) 
            input(type='text', name='foul' required='true' size=3 value=playerData[j+25+16*k])
            input(type='text', name='steal' required='true' size=3 value=playerData[j+26+16*k])
            input(type='text', name='turnover' required='true' size=3 value=playerData[j+27+16*k])
