            input(type='text', name='point' required='true' size=3 value=playerData[j+29+16*k])
            input(type='text', name='rebound' required='true' size=3 value=playerData[j+23+16*k])
            input(type='text', name='assist' required='true' size=3 value=playerData[j+24+16*k])
            input(type='text', name='zhengfuzhi' required='true' size=3 value=playerData[j+30+16*k])
            input(type='text', name='game_time' required='true' size=1 value=gameDate.replace('开赛：',''))
            button(type='submit') 提交
            br
